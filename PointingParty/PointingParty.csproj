<Project Sdk="Microsoft.NET.Sdk.Web">

    <PropertyGroup>
        <TargetFramework>net9.0</TargetFramework>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
        <ScopedCssEnabled>false</ScopedCssEnabled>
        <DisableScopedCssBundling>true</DisableScopedCssBundling>
        <StaticWebAssetFingerprintingEnabled>true</StaticWebAssetFingerprintingEnabled>
        <StaticWebAssetsFingerprintContent>true</StaticWebAssetsFingerprintContent>
    </PropertyGroup>

    <ItemGroup>
        <ProjectReference Include="..\PointingParty.Client\PointingParty.Client.csproj"/>
        <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.Server" Version="9.0.0" />
    </ItemGroup>

</Project>
